first_Number=int(input("Enter the First Number"))
second_Number=int(input("Enter the Second Number"))

sum1=0
sum2=0
i=1
j=1
while i < first_Number:
    if first_Number % i == 0:
        sum1=sum1+i
    i+=1

while j < second_Number:
    if second_Number % j == 0:
        sum2=sum2+j
    j+=1
        
if sum1 == second_Number and sum2 == first_Number :
    print("Yes , The Number is Amicable:")
else:
    print("No, it is not Amicable Number")
